{
  "Version": "2012-10-17",
  "Id": "example-ID",
  "Statement": [
    {
      "Sid": "example-statement-ID",
      "Effect": "Allow",
      "Principal": {
        "Service": "s3.amazonaws.com"
      },
      "Action": "SNS:Publish",
      "Resource": "arn:aws:sns:us-west-2:433744079960:MyTopic",
      "Condition": {
        "StringEquals": {
          "aws:SourceAccount": "433744079960"
        },
        "ArnLike": {
          "aws:SourceArn": "arn:aws:s3:::user-bucket-0001"
        }
      }
    }
  ]
}